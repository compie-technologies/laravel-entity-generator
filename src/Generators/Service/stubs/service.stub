<?php

namespace App\Models\Entities\Services\Dummy;

use App\Models\Entities\Repositories\Dummy\DummyInterface;

class DummyService
{
    protected $dummyRepo;

    public function __construct(DummyInterface $dummyRepo)
    {
        $this->dummyRepo = $dummyRepo;
    }

    public function get($dummyId)
    {
        return $this->dummyRepo->getById($dummyId);
    }
}